# Environment: development, production, testing
DOMINO_ENV=development

# API Configuration
DOMINO_HOST=0.0.0.0
DOMINO_PORT=8000
DOMINO_RELOAD=true
DOMINO_LOG_LEVEL=info

# Security
DOMINO_CORS_ORIGINS=*
DOMINO_CORS_CREDENTIALS=false

# Data Storage
DOMINO_USE_REDIS=false
DOMINO_REDIS_URL=redis://localhost:6379/0
DOMINO_REDIS_PREFIX=domino:

# Domino Configuration
DOMINO_SET_EXPIRY_SECONDS=1209600  # 14 days 